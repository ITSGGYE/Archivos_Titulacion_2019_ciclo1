<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".ProfileActivitya">


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="152dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="0dp"
        android:background="#00838F"
        app:srcCompat="@mipmap/configuracion" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="565dp"
        android:layout_below="@+id/imageView2"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="0dp"
        android:orientation="vertical">

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TableRow
                android:id="@+id/uno"
                android:layout_width="match_parent"
                android:layout_height="35dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="1dp"
                    android:text="Id:"
                    android:textAppearance="@style/Base.TextAppearance.AppCompat.Large"
                    android:textColor="#FFFFFF" />

                <TextView
                    android:id="@+id/textViewId"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="0dp"
                    android:text="-1"
                    android:textAppearance="@style/Base.TextAppearance.AppCompat.Large"
                    android:textColor="#FFFFFF" />
            </TableRow>


            <TableRow
                android:layout_width="match_parent"
                android:layout_height="24dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="10dp"
                    android:text="Bienvenido:"
                    android:textAppearance="@style/Base.TextAppearance.AppCompat.Large"
                    android:textColor="#000000"
                    android:textSize="18sp" />

                <TextView
                    android:id="@+id/textViewUsername"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="10dp"
                    android:text=""
                    android:textAppearance="@style/Base.TextAppearance.AppCompat.Large"
                    android:textColor="#00838F"
                    android:textSize="18sp" />
            </TableRow>


            <TableRow
                android:layout_width="match_parent"
                android:layout_height="24dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:text="Email:"
                    android:textAppearance="@style/Base.TextAppearance.AppCompat.Large"
                    android:textColor="#FFFFFF"
                    android:textSize="18sp" />

                <TextView
                    android:id="@+id/textViewEmail"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:text=""
                    android:textAppearance="@style/Base.TextAppearance.AppCompat.Large"
                    android:textColor="#000000"
                    android:textSize="18sp" />
            </TableRow>

        </TableLayout>

        <Button
            android:id="@+id/buttonLogout"
            android:layout_width="208dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="125dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="125dp"
            android:background="#D84315"
            android:backgroundTint="#F05A5A"
            android:backgroundTintMode="src_over"
            android:drawableLeft="@drawable/cuponc"
            android:text=" cerrar sesion admin"
            android:textColor="#FFFFFF"
            android:textSize="14sp" />


        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:layout_marginTop="70dp"
            android:background="#ffffff">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">


                <TextView
                    android:id="@+id/button7"
                    android:layout_width="313dp"
                    android:layout_height="59dp"
                    android:layout_marginLeft="2dp"
                    android:layout_marginTop="2dp"
                    android:background="#D84315"
                    android:onClick="descuentos"
                    android:paddingTop="15dp"
                    android:text="Descuentos"
                    android:textAlignment="center"
                    android:textColor="#FFFFFF" />


                <ImageView
                    android:id="@+id/iconoa"
                    android:layout_width="95dp"
                    android:layout_height="match_parent"
                    android:layout_marginTop="2dp"
                    android:background="#D84315"
                    android:paddingTop="5dp"
                    app:srcCompat="@drawable/chibuiqr"></ImageView>
            </LinearLayout>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/button8"
                    android:layout_width="313dp"
                    android:layout_height="59dp"
                    android:layout_marginLeft="2dp"
                    android:layout_marginTop="2dp"
                    android:background="#D84315"
                    android:onClick="mantenimiento"
                    android:paddingTop="15dp"
                    android:text="matenimiento"
                    android:textAlignment="center"
                    android:textColor="#FFFFFF" />

                <ImageView
                    android:id="@+id/iconoaa"
                    android:layout_width="95dp"
                    android:layout_height="match_parent"
                    android:layout_marginTop="2dp"
                    android:background="#D84315"
                    android:padding="5dp"
                    app:srcCompat="@drawable/eliminar"></ImageView>
            </LinearLayout>


            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">


                <TextView
                    android:id="@+id/button4"
                    android:layout_width="313dp"
                    android:layout_height="59dp"
                    android:layout_marginLeft="2dp"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:background="#D84315"
                    android:onClick="notificaciones"
                    android:paddingTop="15dp"
                    android:text="notificaciones"
                    android:textAlignment="center"
                    android:textColor="#FFFFFF" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                </LinearLayout>

                <ImageView
                    android:id="@+id/iconoaaa"
                    android:layout_width="95dp"
                    android:layout_height="match_parent"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:background="#D84315"
                    android:padding="5dp"
                    app:srcCompat="@drawable/notification"></ImageView>
            </TableRow>
        </TableLayout>
    </LinearLayout>


</RelativeLayout>